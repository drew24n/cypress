{
  "compilerOptions": {
    "strict": true,
    "noEmitOnError": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "pretty": true,
    "module": "CommonJS",
    "target": "ES5",
    "lib": ["ES5", "dom"],
    "types": ["cypress"]
  },
  "include": ["**/*.ts"]
}
